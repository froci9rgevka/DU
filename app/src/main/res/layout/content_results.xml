            android:onClick="onResultButtonClick"
            android:text="@string/content_main_results11" />

        <Button
            android:id="@+id/result_button_2_5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onResultButtonClick"
            android:text="@string/content_main_results12" />

        <Button
            android:id="@+id/result_button_2_6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onResultButtonClick"
            android:text="@string/content_main_results13" />

        <Button
            android:id="@+id/result_button_2_7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onResultButtonClick"
            android:text="@string/content_main_results14" />

        <Button
            android:id="@+id/result_button_2_8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onResultButtonClick"
            android:text="@string/content_main_results15" />
    </LinearLayout>
