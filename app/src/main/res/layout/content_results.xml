        <Button
            android:id="@+id/result_button_2_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onResultButtonClick"
            android:text="@string/content_main_results8" />

        <Button
            android:id="@+id/result_button_2_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onResultButtonClick"
            android:text="@string/content_main_results9" />

        <Button
            android:id="@+id/result_button_2_3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onResultButtonClick"
