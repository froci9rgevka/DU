                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/textView4" android:textStyle="normal|bold"
                android:textAlignment="viewStart"/>
        <Button
                android:text="@string/content_main_results3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/result_button_1_1"
                android:onClick="onResultButtonClick"/>

        <Button
            android:id="@+id/result_button_1_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onResultButtonClick"
            android:text="@string/content_main_results4" />

        <Button
            android:id="@+id/result_button_1_3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onResultButtonClick"
            android:text="@string/content_main_results5" />

        <Button
            android:id="@+id/result_button_1_4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onResultButtonClick"
            android:text="@string/content_main_results6" />
        <TextView
